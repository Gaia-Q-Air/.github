{
    "name": "GAIA-QAI Development Container",
    "image": "mcr.microsoft.com/vscode/devcontainers/base:ubuntu-20.04",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-vscode.cpptools",
        "rust-lang.rust",
        "ms-python.python"
    ],
    "postCreateCommand": "sudo apt-get update && sudo apt-get install -y cmake python3-pip"
}
